pipeline{
    agent any
    tools{
        maven 'M2_HOME'
    }

    stages{
        stage('maven'){
            steps{
                sh 'mvn clean validate compile install test package' 
            }
        }
        stage('upload artifact'){
            steps{
                sh 'curl -u<USERNAME>:<PASSWORD> -T \
                jenkins_backup.tar \
                "http://ec2-44-202-230-69.compute-1.amazonaws.com:8081/artifactory/geolocation/geolocation/geo/bio_${BUILD_ID}.jar"'
            }

        }
    } 
                    
}